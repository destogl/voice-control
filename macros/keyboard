#See /usr/include/X11/keysymdef.h

ALPHA=hotkey:a
ALTER DOWN=hotkey:KeyStrPress Alt_L\n
ALTER SPACER=hotkey:Alt + Tab
ALTER TAB=hotkey:Alt + Tab
ALTER UP=hotkey:KeyStrRelease Alt_L\n
AMPERSAND=hotkey:&
