CONTROL LEFT=hotkey:Control + ArrowLeft
CONTROL RIGHT=hotkey:Control + ArrowRight
CONTROL UP=KeyStrRelease Control_L\n
HYPHEN=hotkey:-
DELETE=hotkey:Del
DELTA=hotkey:d
DOLLAR=hotkey:Shift + $
